@using dym.Model
@{
    ViewData["Title"] = "会员列表";
}
<h1>会员列表</h1>
<ul>
    @foreach (sys_member m in Model)
    {
        <li>@m.name</li>
    }
</ul>
<script src="~/js/jquery-3.1.0.min.js"></script>
<script type="text/javascript">
    $(function () {
        var timeout = setTimeout(
            function () { 
                $("h1").html("Hello Welcome to .net core!");
            },2000
        );
    });
</script>
